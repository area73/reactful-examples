{
  "name": "reactful-examples",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "validate": "npm-run-all --parallel eslint test",
    "precommit": "npm run eslint",
    "prepush": "npm run validate",
    "eslint": "eslint \"src/**/*.js\"",
    "format": "prettier-eslint \"src/**/*.js\" \"src/**/*.scss\" --write",
    "start": "concurrently \"yarn run dev-server\" \"yarn run dev-bundle\"",
    "test": "jest",
    "test-watch": "jest --watch",
    "dev-server": "cross-env NODE_PATH=./src nodemon --exec \"babel-node src/server/server.js\" --ignore .reactful.json --ignore public/",
    "dev-bundle": "webpack -wd",
    "verify-tests": "jest --coverage",
    "build-react": "cross-env NODE_ENV=production webpack --progress -p",
    "build-node": "babel src -d build --config-file ./babel-node.config.js --copy-files",
    "build-all": "yarn install && yarn run build-react && yarn run build-node",
    "prod-start": "cross-env NODE_ENV=production NODE_PATH=./build pm2 start -i max build/server/server.js --update-env --name poco-cmsProd",
    "prod-stop": "pm2 stop poco-cmsProd",
    "prod-reload": "pm2 reload --update-env poco-cmsProd",
    "prod-logs": "pm2 logs --update-env poco-cmsProd",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest": {
    "modulePaths": [
      "./src"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>src/setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-loader": "^8.0.5",
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "ejs": "^2.6.1",
    "express": ">=4.16.4",
    "mini-css-extract-plugin": "^0.5.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.11.0",
    "pm2": "^3.3.1",
    "react": "16",
    "react-dom": ">=16.8.4",
    "regenerator-runtime": "^0.13.1",
    "sass-loader": "^7.1.0",
    "serialize-javascript": "^1.6.1",
    "style-loader": "^0.23.1",
    "webpack": "4",
    "webpack-chunk-hash": "^0.6.0",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "@babel/node": "^7.2.2",
    "@storybook/addon-actions": "^5.0.1",
    "@storybook/addon-backgrounds": "^5.0.1",
    "@storybook/addon-info": "^5.0.1",
    "@storybook/addon-knobs": "^5.0.1",
    "@storybook/addon-links": "^5.0.1",
    "@storybook/addon-viewport": "^5.0.1",
    "@storybook/react": "^5.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.3.1",
    "concurrently": "^4.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "eslint": "^5.15.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.5.0",
    "husky": "^1.3.1",
    "jest": "^24.3.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "latest",
    "reactful": "^2.0.5"
  }
}
